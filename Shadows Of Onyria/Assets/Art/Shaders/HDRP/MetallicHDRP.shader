				input.isFrontFace = IS_FRONT_VFACE( packedInput.cullFace, true, false);
				#elif SHADER_STAGE_FRAGMENT
				#if defined(ASE_NEED_CULLFACE)
				input.isFrontFace = IS_FRONT_VFACE( packedInput.cullFace, true, false );
				#endif
				#endif
				half isFrontFace = input.isFrontFace;

				PositionInputs posInput = GetPositionInput(input.positionSS.xy, _ScreenSize.zw, input.positionSS.z, input.positionSS.w, input.positionRWS);
				float3 V = GetWorldSpaceNormalizeViewDir(input.positionRWS);
				SurfaceData surfaceData;
				BuiltinData builtinData;

				GlobalSurfaceDescription surfaceDescription = (GlobalSurfaceDescription)0;
				float3 appendResult82 = (float3(_Tint1.r , _Tint1.g , _Tint1.b));
				float2 appendResult15 = (float2(_TileandOffset.x , _TileandOffset.y));
				float2 appendResult14 = (float2(_TileandOffset.z , _TileandOffset.w));
				float2 texCoord17 = packedInput.ase_texcoord5.xy * appendResult15 + appendResult14;
				float3 ase_worldBitangent = packedInput.ase_texcoord6.xyz;
				float3 tanToWorld0 = float3( tangentWS.xyz.x, ase_worldBitangent.x, normalWS.x );
				float3 tanToWorld1 = float3( tangentWS.xyz.y, ase_worldBitangent.y, normalWS.y );
				float3 tanToWorld2 = float3( tangentWS.xyz.z, ase_worldBitangent.z, normalWS.z );
				float3 ase_tanViewDir =  tanToWorld0 * V.x + tanToWorld1 * V.y  + tanToWorld2 * V.z;
				ase_tanViewDir = normalize(ase_tanViewDir);
				float2 OffsetPOM20 = POM( _MaskMap, texCoord17, ddx(texCoord17), ddy(texCoord17), normalWS, V, ase_tanViewDir, 8, 8, _HeightScale, 0, _MaskMap_ST.xy, float2(0,0), 0 );
				float2 TileUV21 = OffsetPOM20;
				float4 tex2DNode80 = tex2D( _BaseMap, TileUV21 );
				float3 temp_output_85_0 = ( appendResult82 * (tex2DNode80).rgb );
				float4 tex2DNode59 = tex2D( _MaskMap, TileUV21 );
				float finalOcclusion64 = pow( abs( tex2DNode59.g ) , _OcclusionStrength1 );
				float3 finalBaseColor88 = (( _UseOcclusionInAlbedo1 )?( ( finalOcclusion64 * temp_output_85_0 ) ):( temp_output_85_0 ));
				
				float3 unpack96 = UnpackNormalScale( tex2D( _NormalMap, TileUV21 ), _NormalIntensity1 );
				unpack96.z = lerp( 1, unpack96.z, saturate(_NormalIntensity1) );
				float3 finalNormal98 = unpack96;
				
				float3 BentNormalsFinal125 = (tex2D( _BentNormals, TileUV21 )).rgb;
				
				float finalMetallic69 = ( _MetallicIntensity1 * tex2DNode59.r );
				
				float4 color100 = IsGammaSpace() ? float4(0,0,0,0) : float4(0,0,0,0);
				float4 finalEmission105 = ( ( (( _UseEmission1 )?( tex2D( _EmissiveMap, TileUV21 ) ):( color100 )) * _EmissionTint ) * _Emission_Gauge );
				
				float finalSmoothness75 = ( tex2DNode59.a * (( _InvertSmoothness1 )?( ( 1.0 - _SmoothnessIntensity1 ) ):( _SmoothnessIntensity1 )) );
				
				float finalOpacity90 = ( _OpacityIntensity1 * tex2DNode80.a );
				
				surfaceDescription.Albedo = finalBaseColor88;
				surfaceDescription.Normal = finalNormal98;
				surfaceDescription.BentNormal = BentNormalsFinal125;
				surfaceDescription.CoatMask = 0;
				surfaceDescription.Metallic = finalMetallic69;

				#ifdef _MATERIAL_FEATURE_SPECULAR_COLOR
				surfaceDescription.Specular = 0;
				#endif

				surfaceDescription.Emission = finalEmission105.rgb;
				surfaceDescription.Smoothness = finalSmoothness75;
				surfaceDescription.Occlusion = finalOcclusion64;
				surfaceDescription.Alpha = finalOpacity90;

				#ifdef _ALPHATEST_ON
				surfaceDescription.AlphaClipThreshold = _AlphaCutoff;
				#endif

				#ifdef _ALPHATEST_SHADOW_ON
